<wizard menus="Interactivity">  <pageWizard remove="true" duplicate="false">    <newNodes>      <textMatch><![CDATA[<textMatch name="Enter Page Title" text="Provide Instructions here" maxAttempts="3" correctMessage="Correct answers are shown" submitBtnTxt="Submit" />]]></textMatch>    </newNodes>  </pageWizard>  <!-- MATCHING TEXTS============================ -->  <textMatch menu="Interactivity" menuItem="Matching Texts" hint="An interaction for the user to match chunks of text." icon="icConnect" thumb="thumbs/textMatch.jpg" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Instructions" type="TextArea" height="100"/>		<maxAttempts label="Attempts Before Hint" type="NumericStepper" min="1" max="99" step="1" defaultValue="3"/>	    <correctMessage label="Correct Message" type="TextInput" wysiwyg="true" divider="true" language="true" />    <submitBtnTxt label="Check Button Label" type="TextInput" wysiwyg="true" language="true" />    <submitBtnTip label="Check Button Tip" type="textInput" language="true" deprecated="This property is no longer required"/>	    <submitBtnWidth label="Check Button Width" type="NumericStepper" min="80" max="250" step="10" width="100" defaultValue="80" language="true" deprecated="Width is now set automatically"/>		    <narrationNavigate label="Navigate on Narration" type="CheckBox" defaultValue="false" optional="true"/>    <playNarration label="Auto-Play Narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media" optional="true"/>    <navSetting label="Navigation Buttons" options="All,Back and Next,Back Only,Next Only,None" type="ComboBox" width="120" data="all,backnext,backonly,nextonly,none" defaultValue="all" optional="true"/>    <pageID label="Page ID" defaultValue="Unique ID for this page" type="TextInput" optional="true" />	    <newNodes>      <sentence><![CDATA[<sentence name="Sentence" p1="Write part one of your sentence here." p2="Write part two of your sentence here" hint="Provide a hint"/>]]></sentence>    </newNodes>  </textMatch>  <sentence menuItem="Sentence" icon="icPageWhiteText" remove="true" max="5">    <p1 label="Part 1" type="TextArea" height="50"/>    <hint label="Hint" type="TextArea" height="50"/>    <p2 label="Part 2" type="TextArea" height="50"/>	<audioFeedback label="Audio Feedback" type="media" optional="true"/>  </sentence></wizard>