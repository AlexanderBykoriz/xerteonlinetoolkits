<wizard menus="Games">  <pageWizard remove="true" duplicate="false">    <newNodes>      <wordsearch><![CDATA[<wordsearch name="Enter Page Title" text="Enter Page text" words="Enter words here, placing each on a new line"	  />]]></wordsearch>    </newNodes>  </pageWizard>  <!-- WORDSEARCH============================== -->  <wordsearch menu="Games" menuItem="Wordsearch" hint="A wordsearch game. Create a list of words for the user to find." icon="icPageWhite" thumb="thumbs/wordsearch.jpg" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <words label="Words" type="script" height="100"/>	<timer label="Page Timer" defaultValue="60" type="NumericStepper" min="0" max="1200" step="1" width="100" optional="true"/>	<order label="Word Ordering" options="None,Alphabetical,Random" type="ComboBox" width="120" data="none,alphabetical,random" defaultValue="none" optional="true"/>  </wordsearch></wizard>