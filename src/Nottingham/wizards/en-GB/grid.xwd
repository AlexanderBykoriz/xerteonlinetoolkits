<wizard menus="Interactivity">  <pageWizard remove="true" duplicate="false">    <newNodes>	  <grid><![CDATA[<grid name="Enter Page Title" text="Enter text for the page here" feedback="Enter feedback here" data="1|2|3" constrain="none" style="row" align="left" drag="insert" checkBtnTxt="Check Answers" resetBtnTxt="Reset" />]]></grid>    </newNodes>    </pageWizard>  <!-- GRID=========================== -->  <grid menu="Interactivity" menuItem="Sortable Grid" hint="An interaction allowing the user to sort items on a grid." icon="icGrid" thumb="thumbs/grid.jpg" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true"/>	<text label="Page Text" type="TextArea" height="150"/>	<feedback label="Feedback" type="TextArea" height="150"/>	<data label="Table Data" newRow="data,data" newCol="data" type="DataGrid" height="190" width="390" columns="2" editable="1,1" controls="1" addCols="true" showHeaderRow="false" rendererOptions="none,none"/>	<constrain label="Constrain Labels" type="ComboBox" options="Off,On (Row),On (Column)" data="none,row,col"/>	<style label="Borders" type="ComboBox" options="None,Full Grid,Simple,Horizontal Only,Vertical Only,Outside Border" data="none,full,simple,horizontal,vertical,outside"/>	<align label="Text Align" type="ComboBox" options="Left,Right,Top" data="left,right,top" defaultValue="left" width="100"/>	<drag label="Drop Label Action" type="ComboBox" options="Insert,Replace" data="insert,replace"/>		<url label="CSV File" type="Media" optional="true"/>	<fixedRows label="Fixed Rows" type="TextInput" defaultValue="Enter numbers of any fixed rows here (e.g. 1,3)" optional="true"/>	<fixedCols label="Fixed Columns" type="TextInput" defaultValue="Enter numbers of any fixed columns here (e.g. 1,3)" optional="true"/>	<fixedCells label="Fixed Labels" type="TextInput" defaultValue="Enter reference to fixed individual cells here (e.g. 1,3|2,5)" optional="true"/>	<header label="Header Row/Column" options="None,First Row,First Column,First Row and Column" type="ComboBox" data="none,row,col,both" defaultValue="none" optional="true"/>    <shadeHeader label="Shade Header Row/Column" type="Checkbox" defaultValue="true" optional="true"/>    <shade label="Shade Alternate Rows" type="Checkbox" defaultValue="true" optional="true"/>		<checkBtnTxt label="Check Button Label" type="TextInput" wysiwyg="true" language="true"/>	<resetBtnTxt label="Reset Button Label" type="TextInput" wysiwyg="true" language="true"/>	    <narrationNavigate label="Navigate on Narration" type="CheckBox" defaultValue="false" optional="true"/>    <playNarration label="Auto-Play Narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media" optional="true"/>    <navSetting label="Navigation Buttons" options="All,Back and Next,Back Only,Next Only,None" type="ComboBox" width="120" data="all,backnext,backonly,nextonly,none" defaultValue="all" optional="true"/>    <pageID label="Page ID" defaultValue="Unique ID for this page" type="TextInput" optional="true"/>	  </grid></wizard>